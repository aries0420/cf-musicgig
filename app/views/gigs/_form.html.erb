<%= form_for @gig, :html => {:multipart => true} do |f| %>
  <div>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </div>
  <div>
    <%= f.file_field :cover_image %>
    <%= f.hidden_field :retained_cover_image %>
  </div>
  <%= f.submit %>
<% end %><br>

<%= link_to "Back To Dashboard", dashboard_path %>
